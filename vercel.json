{
  "version": 2,
  "builds": [
    {
      "src": "simple-server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/simple-server.js"
    },
    {
      "src": "/simple-scheduler.html",
      "dest": "/simple-scheduler.html"
    },
    {
      "src": "/simple-scheduler.js",
      "dest": "/simple-scheduler.js"
    },
    {
      "src": "/exports/(.*)",
      "dest": "/simple-server.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/simple-server.js"
    }
  ],
  "functions": {
    "simple-server.js": {
      "maxDuration": 30
    }
  }
}